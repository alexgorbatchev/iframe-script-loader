{
  "name": "iframe-script-loader",
  "version": "1.0.0",
  "main": "./lib/iframe-script-loader.js",
  "description": "Allows you to load multiple scripts in an IFRAME sandbox.",
  "keywords": [
    "client",
    "iframe",
    "sandbox",
    "script-loader"
  ],
  "scripts": {
    "pretest": "browserify --outfile=lib/iframe-script-loader.js --entry=src/iframe-script-loader.coffee --extension=.coffee --transform=coffeeify --standalone='iframe-script-loader'",
    "test": "./node_modules/karma/bin/karma start --single-run --no-auto-watch",
    "dev": "./node_modules/karma/bin/karma start --no-single-run --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:alexgorbatchev/iframe-script-loader.git"
  },
  "license": "MIT",
  "devDependencies": {
    "browserify": "^4.1.6",
    "coffee-errors": "^0.8.5",
    "coffee-script": "^1.7.1",
    "coffeeify": "^0.6.0",
    "es5-shim": "^3.4.0",
    "karma": "^0.12.16",
    "karma-bro": "^0.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.1.3",
    "karma-phantomjs-launcher": "^0.1.4",
    "run-parallel": "^1.0.0"
  },
  "dependencies": {
    "iframe-script": "^1.0.0",
    "run-parallel-object": "^1.0.1",
    "scriptloader": "git://github.com/alexgorbatchev/scriptloader#patch-2"
  }
}
